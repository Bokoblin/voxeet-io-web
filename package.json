{
  "name": "voxeet-io",
  "version": "1.0.4",
  "private": true,
  "main": "./src/main.js",
  "scripts": {
    "dist": "npm run build:electron && ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true build",
    "bootstrap": "yarn link @voxeet/voxeet-web-sdk && yarn link @voxeet/media-engine && yarn link @voxeet/react-components",
    "start": "webpack-dev-server -d --hot --inline --content-base",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js --progress -p",
    "build:electron": "ELECTRON=true npm run build"
  },
  "author": "Voxeet",
  "contributors": [
    "MARTIN Valentin <vmartin@voxeet.com>",
    "GOURGUES Thomas <tgourgues@voxeet.com>"
  ],
  "build": {
    "appId": "io.voxeet.app",
    "productName": "Voxeet",
    "extends": null,
    "files": [
      "dist/*.svg",
      "dist/*.ttf",
      "dist/index.html",
      "dist/renderer.js",
      "dist/bundle.js",
      "src/main.js",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150
        }
      ]
    },
    "pkg": {
      "allowCurrentUserHome": false,
      "allowAnywhere": false
    },
    "mac": {
      "target": [
        "pkg"
      ],
      "icon": "public/icon.png",
      "extraResources": [
        "libraries"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.png"
    }
  },
  "dependencies": {
    "@trodi/electron-splashscreen": "^0.3.4",
    "@voxeet/react-components": "^0.31.4",
    "@voxeet/voxeet-web-sdk": "^1.9.0",
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "16.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^5.0.0",
    "react-scripts": "1.1.4",
    "redux": "3.7.2",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "electron": "^3.0.5",
    "electron-builder": "^20.38.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.5.9",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "@voxeet/voxeet-web-sdk": "^1.9.0-alpha.16",
    "express": "^4.0.0"
  },
  "files": [
    "README.md",
    "dist/"
  ]
}
